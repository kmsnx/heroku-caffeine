{
  "name": "heroku-caffeine",
  "version": "1.0.0",
  "description": "keeps your Heroku dynos awake",
  "main": "src/index.js",
  "scripts": {
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "test": "jest",
    "test-coverage": "npm run test -- --coverage",
    "lint": "eslint './src/**/*.{js,jsx}'",
    "lint-fix": "eslint --fix './src/**/*.{js,jsx}'",
    "snyk": "snyk test",
    "snyk-fix": "snyk protect",
    "deploy": "npm publish",
    "deploy-beta": "npm publish --tag beta",
    "codecov": "./node_modules/.bin/codecov --token=$CODECOV_TOKEN",
    "codeclimate": "./node_modules/.bin/codeclimate-test-reporter < ./coverage/lcov.info",
    "dev": "node src/demo.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcinxkaminski/heroku-caffeine.git"
  },
  "keywords": [
    "heroku",
    "dynos",
    "awake",
    "caffeine"
  ],
  "author": "Marcin Kaminski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcinxkaminski/heroku-caffeine/issues"
  },
  "homepage": "https://github.com/marcinxkaminski/heroku-caffeine#readme",
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "snyk": "^1.262.2",
    "codecov": "^3.6.1",
    "codeclimate-test-reporter": "^0.5.1"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  }
}
